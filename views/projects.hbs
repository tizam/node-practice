{{> header}}
<div class="row">
{{#each projects}}
  <div class="col-md-4">
      <div class="card mb-4">
        <img src="{{projectImg}}" alt="img" class="card-img-top">
        <div class="card-body">
          <h4 class="card-title">{{projectTitle}}</h4>
          <p class="card-text">{{projectDesc}}</p>
          <a href="{{url}}" class="btn btn-primary">Read more</a>
        </div>
      </div>
    </div>
{{/each}}
</div>

{{> footer}}